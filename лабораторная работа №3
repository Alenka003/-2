#include <iostream>
#include <Windows.h>
#include <string>
#include <stralign.h>

using namespace std;

int main()
{
	char text[50];
	char words[10][10];

	int n = 0;
	int k = 0;

	cin.get(text, 50);
	cout << endl;

	memset(words, 0, sizeof(words[0]) * 10);

	for (int i = 0; i < 50; i++) {
		if (text[i] == ' \0') {
			break;
		}
		if (text[i] != ' ') {
			words[n][k] = text[i];
			k++;
		}
		else {
			n++;
			k = 0;
		}
	}

	char c[] = "EYUIOAeyuioa";
	for (int i = 0; i < 10; i++) {
		for (int j = 0; j < 12; j++) {
			if (words[i][0] == c[j]) {
				cout << words[i] << ' \n';
				break;
			}
		}
	}

	return 0;
}â€ƒ
